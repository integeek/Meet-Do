<IfModule mod_rewrite.c>
RewriteEngine On

RewriteBase /view/Page/

RewriteRule ^InfoPerso/([0-9]+)/([0-9]+)$ InfoPerso.php?id=$1&cle=$2 [L,QSA]

RewriteRule ^Activite/([0-9]+)$ Activite.php?id=$1 [L,QSA] 

RewriteRule ^pageMeeter/([0-9]+)$ pageMeeter.php?id=$1 [L,QSA]

RewriteRule ^([^\.]+)$ $1.php [NC,L]
</IfModule>